// Write a C++ program to demonstrate use of Returning Object as argument.

#include<iostream>
using namespace std;
class student
{
	public:
		double marks1,marks2;
};
student cratestudent()
{
	student student;
	student.marks1 = 90.5;
	student.marks2 = 70.8;
	
      cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
	cout<<"Marks 1 = "<<student.marks1<<endl;
	cout<<"Marks 2 = "<<student.marks2<<endl;
	return student;
}
int main()
{
	student student1;
	student1 = createstudent();
	return 0;
}
